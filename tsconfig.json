{
  "compilerOptions": {
    "strict": true, //타입스크립의 기능을 얼마나 엄격하게 적용해서 변환할 것인지
    "target": "ES5", //컴파일된 자바스크립트의 문법 체계를 설정
    "module": "CommonJS",
    "alwaysStrict": true,
    "noImplicitAny": true, //Any를 명확하게 해야함
    "noImplicitThis": true,
    "sourceMap": true,
    "downlevelIteration": true
  }
}

